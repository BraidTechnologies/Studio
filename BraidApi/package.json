    "axios-retry": "^4.4.2",
    "crypto": "^1.0.1",
    "qs": "^6.13.0"
  },
  "devDependencies": {
    "@fluidframework/azure-client": "^2.2.0",
    "@fluidframework/azure-service-utils": "^2.2.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "22.x",
    "@types/qs": "^6.9.15",
    "azure-functions-core-tools": "^4.x",
    "expect": "^29.7.0",
    "fluid-framework": "^2.2.0",
    "gpt4-tokenizer": "^1.3.0",
    "mocha": "^10.7.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4"
  }
