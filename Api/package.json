{
  "name": "api",
  "version": "0.1.0",
  "description": "",
  "main": "dist/src/functions/*.js",
  "scripts": {
    "build": "npm run compile && npm run pull_js && npm run pull_js_map",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "pull_js": "cp ../CommonTs/dist/src/*.js CommonTs/src",
    "pull_js_map": "cp ../CommonTs/dist/src/*.js.map CommonTs/src",
    "clean": "rimraf dist",
    "clean_js": "rimraf CommonTs",
    "prestart": "npm run clean && npm run clean_js && npm run compile && md CommonTs && md CommonTs\\src && npm run pull_js && npm run pull_js_map",
    "start": "func start --typescript",
    "test": "mocha --debug-brk --exit --require ts-node/register test/*.test.ts",
    "test-mini": "mocha --debug-brk --exit --require ts-node/register test/*driver*.test.ts",
    "test-warm": "mocha --debug-brk --exit --require ts-node/register test/*enriched*.test.ts",
    "local-fluid": "npx @fluidframework/azure-local-service@latest"
  },
  "dependencies": {
    "@azure/functions": "^4.6.0",
    "@fluidframework/azure-client": "^2.13.0",
    "@fluidframework/tinylicious-client": "^2.13.0",
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0",
    "crypto": "^1.0.1",
    "fluid-framework": "2.12",
    "gpt4-tokenizer": "^1.3.0",
    "lodash": "^4.17.21",
    "pako": "^2.1.0",
    "qs": "^6.14.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@fluidframework/build-common": "^2.0.3",
    "@types/lodash": "^4.17.14",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.7",
    "@types/qs": "^6.9.18",
    "azure-functions-core-tools": "^4.0.6821",
    "eslint": "^9.18.0",
    "expect": "^29.7.0",
    "groq-sdk": "^0.12.0",
    "mocha": "^11.0.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
