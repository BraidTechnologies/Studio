{
   "version": "0.2.0",
   "configurations": [      

      {
         "name": "Attach to Node Functions",
         "type": "node",
         "request": "attach",
         "restart": true,
         "port": 9229,
         "preLaunchTask": "func: host start"
      },
      {
         "type": "node",
         "request": "launch",
         "name": "Mocha Tests",
         "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
         "args": [
           "--debug-brk",
           "--require",
           "ts-node/register",
           "${workspaceFolder}/test/pagestore.test.ts"
   
         ],
         "internalConsoleOptions": "openOnSessionStart",
         "skipFiles": [
           "<node_internals>/**"
         ]
       },      
   ]
}