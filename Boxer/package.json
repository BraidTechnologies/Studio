{
   "name": "boxer",
   "version": "0.1.0",
   "description": "An OpenAI assistant that likes to talk about AI, written using Typescript, Node.js, and the Azure stack. GPT-4 backed with a store of AI Canon documents.",
   "main": "ui/AppEntry.tsx",
   "author": {
      "name": "Jon Verrier"
   },
   "sideEffects": false,
   "scripts": {
      "build": "tsc --build && webpack-cli --config webpack-config.js",
      "clean": "tsc --build --clean",
      "test": "mocha --debug-brk --exit --require ts-node/register test/*.test.ts",
      "test-mini": "mocha --debug-brk --exit --require ts-node/register -g Activity*",
      "cover": "tsc && nyc mocha --exit --require ts-node/register test/*test.ts",
      "new-conversation": "mocha --require ts-node/register scripts/make_new_container.ts"
   },
   "devDependencies": {
      "@fluentui/react-components": "^9.56.5",
      "@fluentui/react-icons-font-subsetting-webpack-plugin": "^2.0.267",
      "@fluidframework/azure-client": "^2.11.0",
      "@types/expect": "^24.3.2",
      "@types/express": "^4.17.21",
      "@types/mocha": "^10.0.10",
      "@types/node": "^20.17.10",
      "@types/react": "18.2.46",
      "@types/react-dom": "18.2.18",
      "@types/react-router-dom": "^5.3.3",
      "crypto-browserify": "^3.12.1",
      "esbuild": "0.24.0",
      "expect": "^29.7.0",
      "fluid-framework": "^2.11.0",
      "jsdom": "^23.2.0",
      "missionlog": "^1.8.8",
      "mocha": "^10.8.2",
      "node-polyfill-webpack-plugin": "^3.0.0",
      "npm-check-updates": "^16.14.20",
      "nyc": "^15.1.0",
      "react": "^18.3.1",
      "react-dom": "^18.3.1",
      "react-router-dom": "^6.28.0",
      "stream-browserify": "^3.0.0",
      "ts-loader": "^9.5.1",
      "ts-mocha": "^10.0.0",
      "ts-node": "^10.9.2",
      "typescript": "^5.7.2",
      "webpack": "^5.97.1",
      "webpack-cli": "^5.1.4"
   },
   "nyc": {
      "include": [],
      "extension": [
         ".ts"
      ],
      "reporter": [
         "text-summary",
         "html"
      ],
      "sourceMap": true,
      "instrument": true
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/BraidTechnologies/Studio/Boxer"
   },
   "engines": {
      "node": ">=20.10.0",
      "npm": ">=10.x"
   },
   "dependencies": {
      "gpt4-tokenizer": "^1.3.0",
      "openai": "^4.76.3",
      "qs": "^6.13.1",
      "realm-web": "^2.0.1"
   }
}
