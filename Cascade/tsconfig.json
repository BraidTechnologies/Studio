{
  "compilerOptions": {
   "outDir": "dist",
   "rootDir": ".",
   "target": "ES5",
   "lib": [ "ES6", "DOM"],
   "strict": true,
   "skipLibCheck": true,
   "forceConsistentCasingInFileNames": true,
   "noImplicitAny": true,
   "strictNullChecks" : true,
   "noEmitOnError": true,
   "removeComments": false,
   "sourceMap": true,
   "jsx": "react"
},
"include": [
   "./src/*.ts",
],
"exclude": [
   "./node_modules"
],  
  "references": [
   {
     "path": "../BraidCommon"
   }    
]  
}